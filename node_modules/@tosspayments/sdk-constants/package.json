{
  "name": "@tosspayments/sdk-constants",
  "version": "1.2.2",
  "description": "Enums and Constants used in Toss Payments SDK.",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "lint:eslint": "yarn run -T eslint '{pages,src}/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint-fix": "yarn lint:eslint --fix",
